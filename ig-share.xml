---
layout: null
---
<?xml version="1.0" encoding="UTF-8"?><rss version="2.0"
	xmlns:content="http://purl.org/rss/1.0/modules/content/"
	xmlns:wfw="http://wellformedweb.org/CommentAPI/"
	xmlns:dc="http://purl.org/dc/elements/1.1/"
	xmlns:atom="http://www.w3.org/2005/Atom"
	xmlns:sy="http://purl.org/rss/1.0/modules/syndication/"
	xmlns:slash="http://purl.org/rss/1.0/modules/slash/"
	xmlns:media="http://search.yahoo.com/mrss/"
	xmlns:georss="http://www.georss.org/georss"
	xmlns:geo="http://www.w3.org/2003/01/geo/wgs84_pos#"
	>
  <channel>
   <title>{{ site.title }} | Ebook Shop termurah dan terlengkap di Indonesia</title>
	<atom:link href="{{ "/rss.xml" | prepend: site.baseurl | prepend: site.url }}" rel="self" type="application/rss+xml" />
	<link>{{ site.url }}{{ site.baseurl }}</link>
	<description>{{ site.description | xml_escape }}</description>
	<lastBuildDate>{{ site.time | date_to_rfc822 }}</lastBuildDate>
	<language>id</language>
	<sy:updatePeriod>hourly</sy:updatePeriod>
	<sy:updateFrequency>1	</sy:updateFrequency>
	<generator>Jekyll v{{ jekyll.version }}</generator>
<image>
	<url>{{ site.url }}{{ site.avatar }}</url>
	<title>{{ site.title }}</title>
	<link>{{ site.url }}{{ site.baseurl }}</link>
	<width>32</width>
	<height>32</height>
</image> 
{% assign posta = site.artikel | sort: "date" | reverse %}
    {% for post in posta %}
<item>
        <title>{{ post.title | xml_escape }}</title>
	{% for cat in post.keyword %}
        <category><![CDATA[{{ cat | xml_escape }}]]></category>
        {% endfor %}
	<link>{{ post.url | prepend: site.baseurl | prepend: site.url }}</link>
	<guid isPermaLink="true">{{ post.url | prepend: site.baseurl | prepend: site.url }}</guid>
	<comments>{{ post.url | prepend: site.baseurl | prepend: site.url }}#comments</comments>
	<dc:creator><![CDATA[{{ site.title }}]]></dc:creator>
  <description><![CDATA[<p>{{ post.description | xml_escape }}</p><p>Artikel dengan judul&nbsp;<a href="{{ post.url | prepend: site.baseurl | prepend: site.url }}" rel="nofollow">{{ post.title }}</a> diterbitkan di <a href="{{ site.url }}" rel="nofollow">{{ site.title }} | {{ site.subjudul }}</a></p> ]]></description>
	<content:encoded><![CDATA[{% capture string_with_newlines %}{{ post.content }}{% endcapture %}{{ string_with_newlines | strip_newlines }}]]></content:encoded>   
  <pubDate>{{ post.date | date_to_rfc822 }}</pubDate>
<media:thumbnail url="https://cqfgydydta.cloudimg.io/{{ post.image | replace: 'https://','' | replace: 'http://','' }}?width=1200&amp;height=1200&amp;wat[1]=1&amp;wat_url[1]=https://raw.githubusercontent.com/ajax-jquery/asset.sabdaliterasi.xyz/main/Picsart_24-02-15_16-31-15-276.png&amp;wat_scale[1]=1000p&amp;wat[2]=1&amp;wat_gravity[2]=south&amp;wat_font[2]=Century-Schoolbook-L-Bold&amp;wat_fontsize[2]=100max&amp;wat_color[2]=850102&amp;wat_pad[2]=15.7p&amp;wat_text[2]={{ post.title | xml_escape }}&amp;wat[3]=1&amp;wat_gravity[3]=south&amp;wat_font[3]=Century-Schoolbook-L-Bold&amp;wat_fontsize[3]=100max&amp;wat_color[3]=850102&amp;wat_pad[3]=13.7p&amp;wat_text[3]={{ post.url | prepend: site.baseurl | prepend: site.url }}" width="1080" height="607"/>
<media:content url="https://cqfgydydta.cloudimg.io/{{ post.image | replace: 'https://','' | replace: 'http://','' }}?width=1200&amp;height=1200&amp;wat[1]=1&amp;wat_url[1]=https://raw.githubusercontent.com/ajax-jquery/asset.sabdaliterasi.xyz/main/Picsart_24-02-15_16-31-15-276.png&amp;wat_scale[1]=1000p&amp;wat[2]=1&amp;wat_gravity[2]=south&amp;wat_font[2]=Century-Schoolbook-L-Bold&amp;wat_fontsize[2]=100max&amp;wat_color[2]=850102&amp;wat_pad[2]=15.7p&amp;wat_text[2]={{ post.title | xml_escape }}&amp;wat[3]=1&amp;wat_gravity[3]=south&amp;wat_font[3]=Century-Schoolbook-L-Bold&amp;wat_fontsize[3]=100max&amp;wat_color[3]=850102&amp;wat_pad[3]=13.7p&amp;wat_text[3]={{ post.url | prepend: site.baseurl | prepend: site.url }}" medium="image"/>
</item>
    {% endfor %}
  </channel>
</rss>
